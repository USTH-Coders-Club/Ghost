{{!-- template-lint-disable no-invalid-interactive --}}
<li class="gh-list-row gh-reports-list-item"
    {{on "mouseover" this.mouseOver}}
    {{on "mouseleave" this.mouseLeave}}
    ...attributes
>
    {{#if this.session.user.isContributor}}
        <a class="permalink gh-list-data gh-post-list-title" target="_blank" rel="noopener noreferrer">
            <h3 class="gh-content-entry-title">
                {{@report.type}} {{svg-jar "external" class="gh-report-caches-list-external"}}
            </h3>
                <p>
                    <span class="gh-content-entry-meta">
                        By <span class="midgrey-l2 fw5">{{report-email-names @report}}</span>
                        â€¢ <span data-tooltip="{{gh-format-report-time @report.created_at "D MMM YYYY"}}">{{gh-format-report-time @report.created_at}}</span>
                    </span>
                </p>
        </a>
    {{/if}}
</li>
